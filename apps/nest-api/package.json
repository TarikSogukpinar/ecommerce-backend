  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development dotenv -e .env.development -- npx prisma generate && nest start --debug --watch",
    "staging": "cross-env NODE_ENV=staging dotenv -e .env.staging -- npx prisma generate && nest start",
    "migrate:dev": "cross-env dotenv -e .env.development -- npx prisma migrate dev --name init",
    "migrate": "cross-env dotenv -e .env.development -- npx prisma migrate dev --name init",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
